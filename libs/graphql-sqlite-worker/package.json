{
  "name": "@fitness-recoder/graphql-sqlite-worker",
  "version": "0.0.1",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./dbWorker": {
      "types": "./dist/worker/dbWorker.d.ts",
      "import": "./dist/worker/dbWorker.mjs",
      "require": "./dist/worker/dbWorker.js"
    },
    "./serviceWorker": {
      "types": "./dist/worker/serviceWorker.d.ts",
      "import": "./dist/worker/serviceWorker.mjs",
      "require": "./dist/worker/serviceWorker.js"
    }
  },
  "files": [
    "./dist/worker/sqlite3.wasm",
    "./dist/worker/sqlite3-opfs-async-proxy.js"
  ],
  "dependencies": {
    "@fitness-recoder/structure": "workspace:*",
    "@graphql-tools/merge": "^9.1.7",
    "@graphql-tools/schema": "^10.0.31",
    "@graphql-tools/utils": "^10.11.0",
    "@nx/vite": "22.3.3",
    "@sqlite.org/sqlite-wasm": "^3.51.1-build2",
    "@tanstack/react-query": "^5.90.16",
    "@yornaath/batshit": "^0.14.0",
    "graphql": "^16.12.0",
    "graphql-request": "^7.4.0",
    "graphql-yoga": "^5.18.0",
    "react": "^19.0.0",
    "vite": "^7.0.0",
    "vite-plugin-dts": "~4.5.0",
    "vite-plugin-graphql-loader": "^4.0.4",
    "vite-plugin-wasm": "^3.5.0"
  },
  "nx": {
    "targets": {
      "build-lib": {
        "executor": "@nx/vite:build",
        "options": {
          "configFile": "libs/graphql-sqlite-worker/vite.config.mts",
          "outputPath": "libs/graphql-sqlite-worker/dist"
        }
      },
      "build-worker": {
        "dependsOn": [
          "build-lib"
        ],
        "executor": "@nx/vite:build",
        "options": {
          "generatePackageJson": false,
          "configFile": "libs/graphql-sqlite-worker/vite.config.worker.mts",
          "outputPath": "libs/graphql-sqlite-worker/dist/worker"
        }
      },
      "build": {
        "dependsOn": [
          "build-worker",
          "build-lib"
        ],
        "executor": "nx:noop"
      }
    }
  }
}
